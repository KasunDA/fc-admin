[Unit]
Description=Fleet Commander virtual session controller
After=network.target remote-fs.target nss-lookup.target

[Service]
ExecStart=@PRIVLIBEXECDIR@/fleet_commander_controller.py --configuration  @XDGCONFIGDIR@/fleet-commander-controller.conf

StandardOutput=syslog
StandardError=inherit

[Install]
Alias=fleet-commander-controller.service
