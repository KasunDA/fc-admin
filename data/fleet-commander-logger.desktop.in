[Desktop Entry]
Type=Application
Name=Fleet Commander Session Logger
Icon=preferences-desktop-screensaver
Exec=@PRIVLIBEXECDIR@/fleet_commander_logger.js --configuration @XDGCONFIGDIR@/fleet-commander-logger.conf
OnlyShowIn=GNOME;
NoDisplay=true
X-GNOME-Autostart-Phase=Applications
X-GNOME-Autostart-Notify=true
